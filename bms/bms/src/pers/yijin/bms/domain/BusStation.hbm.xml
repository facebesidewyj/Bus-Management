<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pers.yijin.bms.domain.BusStation" table="bus_station">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="busName" type="java.lang.String">
            <column name="busName"  />
        </property>
        <property name="stationID" type="java.lang.Integer">
            <column name="stationID" />
        </property>
        <property name="stationOrder" type="java.lang.Integer">
        	<column name="stationOrder"></column>
        </property>
    </class>
</hibernate-mapping>
